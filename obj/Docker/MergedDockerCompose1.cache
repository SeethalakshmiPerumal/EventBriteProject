networks:
  backend: {}
  frontend: {}
services:
  event:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\EventCatalogAPI
      dockerfile: Dockerfile
      target: build
    container_name: eventapi
    depends_on:
    - mssqlserver
    entrypoint: tail -f /dev/null
    environment:
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseName: EventDb
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      DatabaseUserPassword: EventApi(!)
      ExternalEventBaseUrl: http://localhost:5060
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/eventapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/EventCatalogAPI.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5060
      target: 80
    volumes:
    - C:\Users\seeth\source\repos\EventBriteAPI\EventCatalogAPI:/app:rw
    - C:\Users\seeth\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\seeth\.nuget\packages:/root/.nuget/packages:ro
  mssqlserver:
    container_name: eventdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: EventApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: null
    ports:
    - published: 1448
      target: 1433
  tokenserver:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\TokenServiceApi
      dockerfile: Dockerfile
      target: base
    container_name: tokenserviceapi1
    depends_on:
    - mssqlserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      MvcClient: http://localhost:5045
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/tokenapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/TokenServiceApi.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5015
      target: 80
    volumes:
    - C:\Users\seeth\source\repos\EventBriteAPI\TokenServiceApi:/app:rw
    - C:\Users\seeth\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\seeth\.nuget\packages:/root/.nuget/packages:ro
  webmvc:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\webmvc
      dockerfile: Dockerfile
      target: build
    container_name: webshop1
    depends_on:
    - event
    - tokenserver
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CallBackUrl: http://localhost:5045
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventUrl: http://event
      IdentityUrl: http://192.168.0.7:5015
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: events/webmvc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/WebMvc.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      frontend: null
    ports:
    - published: 5045
      target: 80
    volumes:
    - C:\Users\seeth\source\repos\EventBriteAPI\webmvc:/app:rw
    - C:\Users\seeth\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\seeth\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'