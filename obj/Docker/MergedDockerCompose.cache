networks:
  backend: {}
  frontend: {}
services:
  event:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\EventCatalogAPI
      dockerfile: Dockerfile
    container_name: eventapi
    depends_on:
    - mssqlserver
    environment:
      DatabaseName: EventDb
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      DatabaseUserPassword: EventApi(!)
      ExternalEventBaseUrl: http://localhost:5060
    image: events/eventapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5060
      target: 80
  mssqlserver:
    container_name: eventdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: EventApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: null
    ports:
    - published: 1448
      target: 1433
  tokenserver:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\TokenServiceApi
      dockerfile: Dockerfile
    container_name: tokenserviceapi1
    depends_on:
    - mssqlserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      MvcClient: http://localhost:5045
    image: events/tokenapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5015
      target: 80
  webmvc:
    build:
      context: C:\Users\seeth\source\repos\EventBriteAPI\webmvc
      dockerfile: Dockerfile
    container_name: webshop1
    depends_on:
    - event
    - tokenserver
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CallBackUrl: http://localhost:5045
      EventUrl: http://event
      IdentityUrl: http://192.168.0.7:5015
    image: events/webmvc
    networks:
      frontend: null
    ports:
    - published: 5045
      target: 80
version: '3.4'